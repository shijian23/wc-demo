/* components/custom-nav-bar/custom-nav-bar.wxss */
.navRoot{
  width: 100vw;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background:#07C99B;
}
.navTitle{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:38rpx;
  color: #fff;
}
.navRoot .back{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 20rpx;
  font-size: 32rpx;
  display: flex;
  align-items: center;
}
.navRoot .back image{
  width: 20rpx;
  height: 36rpx;
  margin-right: 12rpx;
  vertical-align: middle;
}
.navText{
  vertical-align: middle;
}